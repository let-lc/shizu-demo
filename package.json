{
	"name": "shizu",
	"version": "0.1.2",
	"type": "module",
	"private": true,
	"engines": {
		"node": ">=21.0.0"
	},
	"scripts": {
		"dev": "tsx ./src/lib/helpers/init.ts & vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview --port 3000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"init": "tsx ./src/lib/helpers/init.ts",
		"ping": "node ./src/lib/ping/ping.mjs",
		"lint": "eslint . & prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:ui": " eslint ./src/lib/components/ui & prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check ./src/lib/components/ui",
		"format": "eslint --fix . & prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"format:ui": "eslint --fix ./src/lib/components/ui/ & prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write ./src/lib/components/ui/"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.6",
		"@tabler/icons-svelte": "^2.42.0",
		"@types/node": "^20.9.1",
		"@types/tcp-ping": "^0.1.5",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"svelte": "^4.2.4",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"tsx": "^4.1.4",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"zod": "^3.22.4"
	},
	"dependencies": {
		"@types/lodash": "^4.14.201",
		"axios": "^1.6.2",
		"bits-ui": "^0.9.8",
		"chart.js": "^4.4.0",
		"clsx": "^2.0.0",
		"cron-validator": "^1.3.1",
		"cronstrue": "^2.44.0",
		"dayjs": "^1.11.10",
		"formsnap": "^0.4.1",
		"lodash": "^4.17.21",
		"mode-watcher": "^0.1.1",
		"nanoid": "^5.0.3",
		"radix-icons-svelte": "^1.2.1",
		"sveltekit-superforms": "^1.10.2",
		"tailwind-merge": "^2.0.0",
		"tailwind-variants": "^0.1.18",
		"tcp-ping": "^0.1.1"
	}
}
